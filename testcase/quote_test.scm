;; 测试单引号作为 quote 的简写

;; 基本测试
'hello            ;; 应该输出 hello (符号)
'123              ;; 应该输出 123 (数字)
'#t               ;; 应该输出 #t (布尔值)
'#f               ;; 应该输出 #f (布尔值)

;; 列表测试
'(1 2 3)          ;; 应该输出 (1 2 3) (列表)
'()               ;; 应该输出 () (空列表)
'(a b c)          ;; 应该输出 (a b c) (符号列表)

;; 嵌套测试
'(1 (2 3) 4)      ;; 应该输出 (1 (2 3) 4) (嵌套列表)

;; 与完整 quote 形式比较
(quote hello)     ;; 应该与 'hello 相同
(quote (1 2 3))   ;; 应该与 '(1 2 3) 相同

(exit)
